{
  "name": "@djengo/proto-contracts",
  "version": "1.1.0",
  "description": "Shared gRPC proto definitions for DJENGO microservices",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "djengo-proto-setup": "./scripts/copy-to-project.js",
    "djengo-proto-build": "./scripts/build-all.js"
  },
  "scripts": {
    "build": "tsc",
    "build:proto": "ts-node scripts/build-proto.ts",
    "build:all": "npm run build:proto && npm run build",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "prepare": "npm run build",
    "test": "echo \"No tests yet\" && exit 0",
    "proto:generate": "npx  protoc --plugin=protoc-gen-ts_proto=./node_modules/.bin/protoc-gen-ts_proto.cmd --ts_proto_out=./dist/dependencies --ts_proto_opt=nestJs=true --ts_proto_opt=fileSuffix=.pb --ts_proto_opt=outputServices=grpc-js --ts_proto_opt=useOptionals=messages --ts_proto_opt=snakeToCamel=false --proto_path=./proto --proto_path=./node_modules/google-protobuf/google/protobuf ./proto/*.proto",
    "dev": "tsc --watch"
  },
  "files": [
    "dist/",
    "proto/",
    "scripts/",
    "README.md",
    "SETUP.md",
    "*.ts"
  ],
  "keywords": [
    "grpc",
    "protobuf",
    "microservices",
    "djengo",
    "api-contracts",
    "typescript"
  ],
  "author": "DJENGO Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AI-ZeeK/djengo-proto-contracts.git"
  },
  "devDependencies": {
    "@types/google-protobuf": "^3.15.12",
    "@types/node": "^20.11.19",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.9.14",
    "@grpc/proto-loader": "^0.7.10",
    "google-protobuf": "^3.21.2"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./proto/*": "./proto/*"
  }
}
