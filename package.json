{
  "name": "@djengo/proto-contracts",
  "version": "1.0.0",
  "description": "Shared gRPC proto definitions for DJENGO microservices",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run build:ts",
    "build:ts": "tsc",
    "clean": "rimraf dist",
    "prepare": "npm run build",
    "test": "echo \"No tests yet\" && exit 0",
    "dev": "tsc --watch"
  },
  "files": [
    "dist/",
    "proto/",
    "README.md",
    "*.ts"
  ],
  "keywords": [
    "grpc",
    "protobuf",
    "microservices",
    "djengo",
    "api-contracts",
    "typescript"
  ],
  "author": "DJENGO Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AI-ZeeK/djengo-proto-contracts.git"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "rimraf": "^5.0.1",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.9.0",
    "@grpc/proto-loader": "^0.7.0"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./proto/*": "./proto/*"
  }
}
